\PY{c+c1}{\PYZsh{} Cell 1: Problem Statement and Goal (Markdown)}
\PY{l+s+sd}{\PYZdq{}\PYZdq{}\PYZdq{}}
\PY{l+s+sd}{\PYZsh{} Stock Price Prediction Project}
\PY{l+s+sd}{\PYZsh{}\PYZsh{} Task 2: Predict Future Stock Prices (Short\PYZhy{}Term)}

\PY{l+s+sd}{\PYZsh{}\PYZsh{}\PYZsh{} Objective:}
\PY{l+s+sd}{Use historical stock data to predict the next day\PYZsq{}s closing price for Apple Inc. (AAPL).}

\PY{l+s+sd}{\PYZsh{}\PYZsh{}\PYZsh{} Approach:}
\PY{l+s+sd}{1. Load 4 years of historical data (2020\PYZhy{}2023)}
\PY{l+s+sd}{2. Use Open, High, Low, and Volume as features}
\PY{l+s+sd}{3. Train and compare Linear Regression and Random Forest models}
\PY{l+s+sd}{4. Evaluate performance and make next\PYZhy{}day predictions}
\PY{l+s+sd}{\PYZdq{}\PYZdq{}\PYZdq{}}
\end{Verbatim}
